# NOTE: This project uses Apache Ant as the primary build tool.
# Please use 'ant build' instead of 'make'.
# This Makefile is provided for compatibility only.

.PHONY: all clean run

all:
	@echo "Please use 'ant build' to build this project"
	@echo "Or run 'ant' for the default target"

clean:
	@echo "Please use 'ant clean' to clean this project"

run:
	@echo "Please use 'ant run -Dargs=\"your args here\"' to run this project"

